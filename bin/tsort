#!/bin/bash
file="${1-plan.ignore}"
final_file="${2-script.sh}"
echo '#!/bin/bash' > $final_file
resources=$(for line in $(cat $file); do
    line="$(echo $line | cut -d'.' -f 1)"
    echo "${line}"
done | grep google | sort | uniq | sed "s|'||g")
while IFS= read -r resource; do
    cat $file | grep $resource >> $final_file
done <<< "$resources"
