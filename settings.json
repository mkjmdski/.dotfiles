{
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Dracula",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.trimTrailingWhitespace": true,
    "editor.wordWrap": "on",
    "editor.formatOnSave": true,
    "explorer.confirmDelete": false,
    "editor.tabCompletion": "on",
    "editor.cursorStyle": "block-outline",
    "workbench.startupEditor": "newUntitledFile",
    "window.closeWhenEmpty": true,
    "window.restoreFullscreen": true,
    "window.restoreWindows": "all",
    "window.zoomLevel": 0,
    "editor.largeFileOptimizations": false,
    "editor.suggestSelection": "first",
    "files.watcherExclude": {
        "**/dump/**": true,
        "**/img/**": true,
        "**/var/cache/**": true
    },
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "terraform.languageServer": {
        "enabled": true,
        "args": []
    },
    "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.markdownlint": true
    },
    "markdownlint.config": {
        "MD024": false,
        "MD025": false,
        "MD026": false,
        "MD033": false
    },
    "bashIde.explainshellEndpoint": "http://localhost:5000",
    "gitProjectManager.baseProjectsFolders": [
        "~/repos"
    ],
    "[python]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "python.autoComplete.addBrackets": true,
    "python.dataScience.allowImportFromNotebook": false,
    "python.dataScience.addGotoCodeLenses": false,
    "python.dataScience.allowLiveShare": false,
    "python.dataScience.allowInput": false,
    "python.dataScience.askForLargeDataFrames": false,
    "python.dataScience.collapseCellInputCodeByDefault": false,
    "python.dataScience.colorizeInputBox": false,
    "python.dataScience.debugJustMyCode": false,
    "python.dataScience.decorateCells": false,
    "python.dataScience.enableAutoMoveToNextCell": false,
    "python.dataScience.enableCellCodeLens": false,
    "python.dataScience.enabled": false,
    "python.dataScience.enableGather": false,
    "python.dataScience.enablePlotViewer": false,
    "python.formatting.provider": "black",
    "python.jediEnabled": false
}